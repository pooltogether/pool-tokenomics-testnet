{
  "name": "@pooltogether/tokenomics-testnet",
  "version": "1.4.0",
  "main": "index.js",
  "repository": "git@github.com:pooltogether/v4-testnet.git",
  "author": "Brendan Asselstine <mail.asselstine@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "clean": "rm -rf cache/ artifacts/",
    "verify:rinkeby": "hardhat run ./scripts/verify.ts --network rinkeby",
    "verify:mumbai": "hardhat --config hardhat.config.polygon.ts run ./scripts/verify.ts --network mumbai",
    "compile": "hardhat compile",
    "hh": "hardhat",
    "docs": "hardhat deploy-markdown-export --name 'V4 Testnet' --npm-package-name @pooltogether/v4-testnet --github-base-url 'https://github.com/pooltogether/v4-testnet/tree/master' --output-filepath testnet.md",
    "addPrizes": "hardhat run ./scripts/addPrizes.js --network ",
    "disburse": "hardhat run ./scripts/disburse.js --network localhost",
    "arb": "hardhat run ./scripts/arb.js --network localhost",
    "draw": "hardhat run ./scripts/draw.js --network localhost",
    "time": "hardhat run ./scripts/time.js --network localhost",
    "picks": "hardhat run ./scripts/picks.js --network localhost",
    "stake": "hardhat run ./scripts/stake.js --network localhost",
    "deposit": "hardhat run ./scripts/deposit.js --network localhost",
    "yield": "hardhat run ./scripts/yield.js --network localhost",
    "generate-contract-list": "node ./scripts/generateContractList.js",
    "prepare-deploy": "yarn clean && yarn compile",
    "prepack": "yarn generate",
    "reinstall": "rm -rf node_modules/ && yarn cache clean && yarn",
    "format": "prettier --config .prettierrc --write \"**/*.{ts,js}\"",
    "format:file": "prettier --config .prettierrc --write",
    "deploy": "hardhat deploy --network",
    "deploy:rinkeby": "hardhat deploy --network rinkeby",
    "deploy:mumbai": "hardhat deploy --network mumbai",
    "deploy:dry-run": "hardhat node --network hardhat"
  },
  "dependencies": {
    "@chainlink/contracts": "0.4.0",
    "@openzeppelin/contracts": "4.4.2",
    "@pooltogether/draw-calculator-js": "1.0.3-beta.4",
    "@pooltogether/owner-manager-contracts": "1.1.0",
    "@pooltogether/pooltogether-rng-contracts": "1.5.1",
    "@pooltogether/v4-core": "1.4.0-beta.3",
    "@pooltogether/v4-periphery": "1.2.3",
    "@pooltogether/v4-timelocks": "1.1.0",
    "@pooltogether/v4-twab-delegator": "1.0.0",
    "@pooltogether/yield-source-interface": "1.4.0",
    "find": "0.3.0",
    "glob": "7.2.0"
  },
  "devDependencies": {
    "@ethersproject/hardware-wallets": "5.5.0",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-etherscan": "3.0.0",
    "@pooltogether/hardhat-deploy-markdown-export": "0.2.1",
    "@types/find": "0.2.1",
    "@types/node": "17.0.10",
    "chalk": "4.1.2",
    "colors": "1.4.0",
    "debug": "4.3.3",
    "ethers": "5.5.3",
    "hardhat": "2.8.3",
    "hardhat-dependency-compiler": "1.1.2",
    "hardhat-deploy": "0.9.26",
    "hardhat-deploy-ethers": "0.3.0-beta.13",
    "husky": "8.0.1",
    "idx": "2.5.6",
    "luxon": "2.3.0",
    "prettier": "2.5.1",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  },
  "files": [
    "artifacts",
    "deployments/mumbai",
    "deployments/rinkeby",
    "deployments/fuji",
    "contracts.json"
  ]
}
